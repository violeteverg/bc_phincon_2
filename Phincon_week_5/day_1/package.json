{
  "name": "day_1",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "dev": "concurrently \"npm run build:watch\" \"npm run copy-files\" \"npm run serve:watch\"",
    "copy-files": "cp -r ./views ./dist/",
    "serve": "node dist/index.js",
    "serve:watch": "nodemon dist/index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:run": "npx sequelize-cli db:seed --seed"
  },
